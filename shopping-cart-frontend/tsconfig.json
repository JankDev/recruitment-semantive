{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@env": ["src/environments/environment.ts"],
      "@shared": ["src/app/_shared/index.ts"],
      "@shared/*": ["src/app/_shared/*"],
      "@core": ["src/app/_core/index.ts"],
      "@core/*": ["src/app/_core/*"],
      "@order": ["src/app/order/index.ts"],
      "@order/*": ["srd/app/order/*"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
